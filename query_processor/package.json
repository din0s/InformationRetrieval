    "url": "https://github.com/din0s/InformationRetrieval/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "heap": "^0.2.6",
    "natural": "^2.4.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
